{
  "name": "bg3d",
  "version": "0.1.0",
  "description": "An open source THREE.js based multiplayer board game simulator",
  "dependencies": {
    "@zip.js/zip.js": "2.4.12",
    "three": "^0.151.0"
  },
  "devDependencies": {
    "esbuild": "^0.17.6",
    "esbuild-plugin-wasm": "^1.0.0",
    "ts-loader": "^9.4.2",
    "tsify": "^5.0.4",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "esbuild static/js/index.js --bundle --minify --sourcemap --target=chrome58 --outfile=static/bundle.js",
    "prelude": "esbuild static/js/prelude.js --bundle --minify --target=chrome58 --outfile=static/prelude.bundle.js",
    "no_minify": "esbuild static/js/index.js --bundle --sourcemap --target=chrome58 --outfile=static/bundle.js",
    "watch": "esbuild static/js/index.js --watch --bundle --minify --sourcemap --target=chrome58 --outfile=static/bundle.js"
  },
  "author": "tesseractcat",
  "license": "MIT"
}
